{
  "name": "menu-app",
  "version": "1.0.0",
  "description": "A restaurant menu application built with MERN stack",
  "scripts": {
    "dev": "concurrently \"npm run dev:frontend\" \"npm run dev:backend\"",
    "dev:frontend": "cd frontend && npm run dev",
    "dev:backend": "cd backend && npm run dev",
    "build": "cd frontend && npm run build",
    "start": "cd backend && npm start",
    "install:frontend": "cd frontend && npm install",
    "install:backend": "cd backend && npm install",
    "install:all": "npm run install:frontend && npm run install:backend"
  },
  "keywords": [
    "react",
    "nodejs",
    "express",
    "mongodb",
    "mern",
    "tailwind",
    "vite",
    "restaurant",
    "menu"
  ],
  "author": "Halil Taylan Engin",
  "license": "MIT",
  "devDependencies": {
    "concurrently": "^8.2.2"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/HalilTaylanEngin/menu-app.git"
  },
  "bugs": {
    "url": "https://github.com/HalilTaylanEngin/menu-app/issues"
  },
  "homepage": "https://github.com/HalilTaylanEngin/menu-app#readme",
  "dependencies": {
    "@reduxjs/toolkit": "^2.8.2",
    "react-redux": "^9.2.0"
  }
}
